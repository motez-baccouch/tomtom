{% extends 'base.html.twig' %}

{% block body %}
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ social_media.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ social_media.nom }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ social_media.icon }}</td>
            </tr>
            <tr>
                <th>CssClass</th>
                <td>{{ social_media.cssClass }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ social_media.url }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-primary form-btn" href="{{ path('social_media_edit', {'id': social_media.id}) }}">edit</a>
    <br>
    <a class="btn btn-secondary form-btn" href="{{ path('social_media_index') }}">back to list</a>

    {{ include('social_media/_delete_form.html.twig') }}
{% endblock %}
